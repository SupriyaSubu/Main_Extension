{
	"manifest_version": 2,

	"name": "Scraper",
	"description": "Easily scrap the webpage",
	"version": "1.0.1",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": { "scripts": ["popup.js"]},
	"permissions": [
	"contextMenus",
	"tabs",
	"http://*/*",
	"https://*/*"
	], "minimum_chrome_version": "6.0.0.0"
}